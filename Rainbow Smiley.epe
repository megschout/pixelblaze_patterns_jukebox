    "main": "// This only works on a 16x16 matrix.\n\n//  Image helpers\nfunction numImages() { return _images.length; }\nfunction numFrames(image) { return _images[image].length; }\nfunction numRows(image, frame) { return _images[image][frame].length; }\nfunction numColumns(image, frame) { return _images[image][frame][0].length; }\n//  Pixel helpers\nfunction putPixel(image, frame, row, column, value) { _images[image][frame][row][column] = value; }\nfunction getPixel(image, frame, row, column, wrap) { if (column >= numColumns(image,frame)) { if (!wrap) return 0; else column = column % numColumns(image,frame); } return _images[image][frame][row][column]; }\nfunction packRGBA(r, g, b, a) { return (r << 8) + g + (b >> 8) + (a >> 16); }\nfunction _r(pixval) { return (pixval >> 8) & 0xff; }\nfunction _g(pixval) { return pixval & 0xff; }\nfunction _b(pixval) { return (pixval << 8) & 0xff; }\nfunction _a(pixval) { return (pixval << 16) & 0xff; }\n//  Viewport storage\nvar viewportOffsetX = 0, viewportOffsetY = 0, viewportWrap = true;\n// Animation storage\nexport var elapsedTime = 0, loopTime = 50;\nexport function beforeRender(delta) {\n  t1=time(0.03)\n  elapsedTime += delta; if (elapsedTime > loopTime) {\n    elapsedTime = 0;\n    //  Scroll horizontally; wrap when we get to the end.\n    //viewportOffsetX += 1; if (viewportOffsetX >= numColumns(0,0,0)) { viewportOffsetX = 0; }\n  }\n}\n// Rendering functions\nexport function render2D(index, x, y) {\n  dimension = sqrt(pixelCount); row = trunc(x * dimension); col = trunc(y * dimension);\n  pixval = getPixel(0, 0, row + viewportOffsetY, col + viewportOffsetX, viewportWrap); \n  if (_a(pixval) > 0) hsv(t1+index/pixelCount,1,1); else rgb(_r(pixval), _g(pixval), _b(pixval));\n}\n\n\n//  Image storage\nvar _images = array(1); //  allocate image storage\n// Image #0 (16x16) generated from 'smiley16.bmp'\n  //getImage(0) = array(1); // allocate frame storage\n  _images[0] = array(1); // allocate frame storage\n    // Frame #0